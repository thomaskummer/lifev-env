## ---------------------------------------------------------------------------
## Compilers used by Trilinos/Sacado build
## ---------------------------------------------------------------------------

Sacado_CXX_COMPILER=/usr/lib64/openmpi/bin/mpicxx

Sacado_C_COMPILER=/usr/lib64/openmpi/bin/mpicc

Sacado_FORTRAN_COMPILER=/usr/lib64/openmpi/bin/mpif90
Sacado_Fortran_COMPILER=/usr/lib64/openmpi/bin/mpif90


## ---------------------------------------------------------------------------
## Compiler flags used by Trilinos/Sacado build
## ---------------------------------------------------------------------------

## Set compiler flags, including those determined by build type
Sacado_CXX_FLAGS= -O3
Sacado_CXX_COMPILER_FLAGS= -O3

Sacado_C_FLAGS= -O3
Sacado_C_COMPILER_FLAGS= -O3

Sacado_FORTRAN_FLAGS= -O3
Sacado_Fortran_COMPILER_FLAGS= -O3

## Extra link flags (e.g., specification of fortran libraries)
Sacado_EXTRA_LD_FLAGS=-lrt

## This is the command-line entry used for setting rpaths. In a build
## with static libraries it will be empty. 
Sacado_SHARED_LIB_RPATH_COMMAND=
Sacado_BUILD_SHARED_LIBS=OFF

Sacado_LINKER=/usr/bin/ld
Sacado_AR=/usr/bin/ar

## ---------------------------------------------------------------------------
## Set library specifications and paths 
## ---------------------------------------------------------------------------

## The project version number
Sacado_VERSION=11.4.3

## List of package include dirs
Sacado_INCLUDE_DIRS= -I/local/kummerth/lifev-env/lifev-env/trilinos-install/include

## List of package library paths
Sacado_LIBRARY_DIRS= -L/local/kummerth/lifev-env/lifev-env/trilinos-install/lib

## List of package libraries
Sacado_LIBRARIES= -lsacado -lteuchosremainder -lteuchosnumerics -lteuchoscomm -lteuchosparameterlist -lteuchoscore

## Specification of directories for TPL headers
Sacado_TPL_INCLUDE_DIRS= -I/usr/include

## Specification of directories for TPL libraries
Sacado_TPL_LIBRARY_DIRS=

## List of required TPLs
Sacado_TPL_LIBRARIES= /usr/lib64/liblapack.so /usr/lib64/libblas.so

## ---------------------------------------------------------------------------
## MPI specific variables
##   These variables are provided to make it easier to get the mpi libraries
##   and includes on systems that do not use the mpi wrappers for compiling
## ---------------------------------------------------------------------------

Sacado_MPI_LIBRARIES=
Sacado_MPI_LIBRARY_DIRS=
Sacado_MPI_INCLUDE_DIRS=
Sacado_MPI_EXEC=/usr/bin/mpiexec
Sacado_MPI_EXEC_MAX_NUMPROCS=4
Sacado_MPI_EXEC_NUMPROCS_FLAG=-np

## ---------------------------------------------------------------------------
## Set useful general variables 
## ---------------------------------------------------------------------------

## The packages enabled for this project
Sacado_PACKAGE_LIST= Sacado Teuchos TeuchosRemainder TeuchosNumerics TeuchosComm TeuchosParameterList TeuchosCore

## The TPLs enabled for this project
Sacado_TPL_LIST= Boost LAPACK BLAS MPI

## ---------------------------------------------------------------------------
## Deprecated variables
##   These variables from the autotools version of Makefile.export.<package>
##   have been deprecated. To help avoid confusion with projects that haven't
##   migrated to the new version of the variable names when the don't compile.
##   This will put an obvious statement in the link line that these Trilinos
##   Makefile variables are no longer used. Without this there would be a link
##   error with no obvious reason as to why other than the lack of Trilinos libs
##   which would be confusing since the link line in the Makefile would have 
##   stated to put them on the line.
## ---------------------------------------------------------------------------

SACADO_LIBS="The Trilinos variable SACADO_LIBS has been deprecated. Please see the documentation on the Makefile.export capability for instructions on how to transition to the new system."
SACADO_INCLUDES="The Trilinos variable SACADO_INCLUDES has been deprecated. Please see the documentation on the Makefile.export capability for instructions on how to transition to the new system."


