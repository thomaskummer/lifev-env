## ---------------------------------------------------------------------------
## Compilers used by Trilinos/TeuchosRemainder build
## ---------------------------------------------------------------------------

TeuchosRemainder_CXX_COMPILER=/usr/lib64/openmpi/bin/mpicxx

TeuchosRemainder_C_COMPILER=/usr/lib64/openmpi/bin/mpicc

TeuchosRemainder_FORTRAN_COMPILER=/usr/lib64/openmpi/bin/mpif90
TeuchosRemainder_Fortran_COMPILER=/usr/lib64/openmpi/bin/mpif90


## ---------------------------------------------------------------------------
## Compiler flags used by Trilinos/TeuchosRemainder build
## ---------------------------------------------------------------------------

## Set compiler flags, including those determined by build type
TeuchosRemainder_CXX_FLAGS= -O3
TeuchosRemainder_CXX_COMPILER_FLAGS= -O3

TeuchosRemainder_C_FLAGS= -O3
TeuchosRemainder_C_COMPILER_FLAGS= -O3

TeuchosRemainder_FORTRAN_FLAGS= -O3
TeuchosRemainder_Fortran_COMPILER_FLAGS= -O3

## Extra link flags (e.g., specification of fortran libraries)
TeuchosRemainder_EXTRA_LD_FLAGS=-lrt

## This is the command-line entry used for setting rpaths. In a build
## with static libraries it will be empty. 
TeuchosRemainder_SHARED_LIB_RPATH_COMMAND=
TeuchosRemainder_BUILD_SHARED_LIBS=OFF

TeuchosRemainder_LINKER=/usr/bin/ld
TeuchosRemainder_AR=/usr/bin/ar

## ---------------------------------------------------------------------------
## Set library specifications and paths 
## ---------------------------------------------------------------------------

## The project version number
TeuchosRemainder_VERSION=11.4.3

## List of package include dirs
TeuchosRemainder_INCLUDE_DIRS= -I/local/kummerth/lifev-env/lifev-env/trilinos-install/include

## List of package library paths
TeuchosRemainder_LIBRARY_DIRS= -L/local/kummerth/lifev-env/lifev-env/trilinos-install/lib

## List of package libraries
TeuchosRemainder_LIBRARIES= -lteuchosremainder -lteuchoscore

## Specification of directories for TPL headers
TeuchosRemainder_TPL_INCLUDE_DIRS= -I/usr/include

## Specification of directories for TPL libraries
TeuchosRemainder_TPL_LIBRARY_DIRS=

## List of required TPLs
TeuchosRemainder_TPL_LIBRARIES=

## ---------------------------------------------------------------------------
## MPI specific variables
##   These variables are provided to make it easier to get the mpi libraries
##   and includes on systems that do not use the mpi wrappers for compiling
## ---------------------------------------------------------------------------

TeuchosRemainder_MPI_LIBRARIES=
TeuchosRemainder_MPI_LIBRARY_DIRS=
TeuchosRemainder_MPI_INCLUDE_DIRS=
TeuchosRemainder_MPI_EXEC=/usr/bin/mpiexec
TeuchosRemainder_MPI_EXEC_MAX_NUMPROCS=4
TeuchosRemainder_MPI_EXEC_NUMPROCS_FLAG=-np

## ---------------------------------------------------------------------------
## Set useful general variables 
## ---------------------------------------------------------------------------

## The packages enabled for this project
TeuchosRemainder_PACKAGE_LIST= TeuchosRemainder TeuchosCore

## The TPLs enabled for this project
TeuchosRemainder_TPL_LIST= Boost MPI

## ---------------------------------------------------------------------------
## Deprecated variables
##   These variables from the autotools version of Makefile.export.<package>
##   have been deprecated. To help avoid confusion with projects that haven't
##   migrated to the new version of the variable names when the don't compile.
##   This will put an obvious statement in the link line that these Trilinos
##   Makefile variables are no longer used. Without this there would be a link
##   error with no obvious reason as to why other than the lack of Trilinos libs
##   which would be confusing since the link line in the Makefile would have 
##   stated to put them on the line.
## ---------------------------------------------------------------------------

